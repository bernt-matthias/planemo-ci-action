        <token name="@TOOL_VERSION@">0.1.0</token>
        <token name="@VERSION_SUFFIX@">0</token>
    </macros>
    <command detect_errors="exit_code"><![CDATA[
        echo "GALAXY_SLOTS \${GALAXY_SLOTS:-1}" > '$out'
